{
  "name": "bulma-react",
  "version": "0.0.8",
  "description": "Bulma components build with reacjs",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --progress --verbose --colors --display-error-details --config ./webpack/config.lib.js",
    "compile": "babel --presets es2015,react --watch src/components/ --out-dir lib/",
    "test": "mocha --opts mocha.opts",
    "test:watch": "mocha --opts mocha.opts --watch --reporter min"
  },
  "author": "Renan Plathon <plathonrenan@gmail.com>",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/plathon/bulma-react"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plathon/bulma-react/issues"
  },
  "homepage": "https://plathon.github.io/bulma-react",
  "keywords": [
    "react",
    "bulma"
  ],
  "dependencies": {
    "bulma": "0.0.24",
    "classnames": "^2.2.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.0.0",
    "expect": "^1.14.0",
    "expect-jsx": "^2.3.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
